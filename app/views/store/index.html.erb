<p id="notice"><%= notice %></p>
<h1>Alexblack Catalog</h1>
<% @products.each do |product| %>
<div class="entry">
<%= image_tag(product.image1) %>
<h3><%= product.title %></h3>
<%= sanitize(product.description) %>
<div class="price_line">
<span class="price"><%= product.price %></span>
</div>
</div>
<% end %>
